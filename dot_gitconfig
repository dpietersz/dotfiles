[user]
	name = Rio Kierkels
	email = riokierkels@gmail.com

[init]
	defaultBranch = main

[pull]
	rebase = false

[push]
	autoSetupRemote = true

[branch]
	autoSetupMerge = true

[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process

[credential]
        helper = !'/usr/local/bin/devpod' agent git-credentials --port 17190
