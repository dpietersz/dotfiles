keybinds {
    unbind "Ctrl h"
    shared_except "move" "locked" {
        // do not use Ctrl h because
        bind "Ctrl f" { SwitchToMode "Move"; }
    }

    unbind "Ctrl o"
    shared_except "session" "locked" {
        // do not use Ctrl o because
        bind "Ctrl S" { SwitchToMode "Session"; }
    }
}

pane_frames false

themes {
    dracula {
        fg 248 248 242
        bg 40 42 54
        red 255 85 85
        green 80 250 123
        yellow 241 250 140
        blue 98 114 164
        magenta 255 121 198
        orange 255 184 108
        cyan 139 233 253
        black 0 0 0
        white 255 255 255
    }
}

theme "dracula"

