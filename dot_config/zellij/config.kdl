keybinds {
    // disable most sortcuts and lean mostly on tmux bindings
    normal {
      unbind "Ctrl h"
      unbind "Ctrl n"
      unbind "Ctrl o"
      unbind "Ctrl t"
      unbind "Ctrl p"
    }
}

pane_frames false

theme "catppuccin-macchiato"

default_layout "compact"
default_mode "locked"
